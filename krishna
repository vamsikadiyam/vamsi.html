// copy to clipboard
if (document.getElementById("copyButton")) {
  document.getElementById("copyButton").addEventListener("click", function() {
    copyToClipboardMsg(document.getElementById("copyTarget"), "msg");
  });
}

function copyToClipboardMsg(elem, msgElem) {
  var succeed = copyToClipboard(elem);
  var msg;
  if (!succeed) {
    msg = "Copy not supported or blocked."
  } else {
    msg = "Copied to the clipboard!"
  }
  if (typeof msgElem === "string") {
    msgElem = document.getElementById(msgElem);
  }
  msgElem.innerHTML = msg;
  setTimeout(function() {
    msgElem.innerHTML = "";
  }, 2000);
}

function copyToClipboard(elem) {
  // create hidden text element, if it doesn't already exist
  var targetId = "_hiddenCopyText_";
  var isInput = elem.tagName === "INPUT" || elem.tagName === "TEXTAREA";
  var origSelectionStart, origSelectionEnd;
  if (isInput) {
    // can just use the original source element for the selection and copy
    target = elem;
    origSelectionStart = elem.selectionStart;
    origSelectionEnd = elem.selectionEnd;
  } else {
    // must use a temporary form element for the selection and copy
    target = document.getElementById(targetId);
    if (!target) {
      var target = document.createElement("textarea");
      target.style.position = "absolute";
      target.style.left = "-9999px";
      target.style.top = "0";
      target.id = targetId;
      document.body.appendChild(target);
    }
    target.textContent = elem.textContent;
  }
  // select the content
  var currentFocus = document.activeElement;
  target.focus();
  target.setSelectionRange(0, target.value.length);

  // copy the selection
  var succeed;
  try {
    succeed = document.execCommand("copy");
  } catch (e) {
    succeed = false;
  }
  // restore original focus
  if (currentFocus && typeof currentFocus.focus === "function") {
    currentFocus.focus();
  }

  if (isInput) {
    // restore prior selection
    elem.setSelectionRange(origSelectionStart, origSelectionEnd);
  } else {
    // clear temporary content
    target.textContent = "";
  }
  return succeed;
}

var selectedOS = detectOS();

// OS tabs
jQuery('ul.tabs li').click(function() {
  var tab_id = jQuery(this).attr('data-tab');

  jQuery('ul.tabs li').removeClass('current');
  jQuery('.tab-content').removeClass('current');

  jQuery(this).addClass('current');
  jQuery("#" + tab_id).addClass('current');
  
  if(jQuery(this)[0].className.indexOf('MacOS') > -1) {
      selectedOS = 'MacOS'
  }
  if(jQuery(this)[0].className.indexOf('Windows') > -1) {
      selectedOS = 'Windows'
  }
  if(jQuery(this)[0].className.indexOf('Linux') > -1) {
      selectedOS = 'Linux'
  }
  sessionStorage.setItem('selectedOS', selectedOS);
});


// find out os
jQuery(document).ready(function() {
  var OSName = detectOS()
  if (OSName == "Windows") {
    jQuery('ul.tabs li').removeClass('current');
    jQuery('.tab-content').removeClass('current');
    jQuery('ul.tabs li.Windows').addClass("current");
    jQuery('.tab-content.Windows').addClass('current');
  }
  if (OSName == "Linux") {
    jQuery('ul.tabs li').removeClass('current');
    jQuery('.tab-content').removeClass('current');
    jQuery('ul.tabs li.Linux').addClass("current");
    jQuery('.tab-content.Linux').addClass('current');
  }

});

jQuery('#downloadLink').click(function(){
    beginDownload();
})

function setSHA() {
    var selectedOS = sessionStorage.getItem('selectedOS') ? sessionStorage.getItem('selectedOS') : detectOS();
    var sha = ''
    if(selectedOS === 'MacOS') {
        sha = 'b2064623c15cb4ac489ae603f0e0d120aa5f3de6457f5e9fb5eec788f66cc1d6'
    }
    if(selectedOS === 'Windows') {
        sha = 'c9b137fbd8d986a7eabec3cb24912894aa11666564b745f71081943c59579a86'
    }
    if(selectedOS === 'Linux') {
        sha = '9045fb925550c10c410e10ce80b2880ea38f180b625db9029384447ca45b3db3'
    }
    
    jQuery('#copyTarget')[0].innerHTML = sha
}

function beginDownload() {
    var selectedOS = sessionStorage.getItem('selectedOS') ? sessionStorage.getItem('selectedOS') : detectOS();
    
    if(selectedOS === 'MacOS') {
        window.location.href ='https://d3gcsm2x428gz5.cloudfront.net/CodePilot.ai-4.1.1.dmg';
        __gaTracker('send', 'event', 'Click', 'Download', 'Mac Download');
        return false;
    }
    if(selectedOS === 'Windows') {
        window.location.href ='https://d3gcsm2x428gz5.cloudfront.net/CodePilot.ai+Setup+4.1.1.exe';
        __gaTracker('send', 'event', 'Click', 'Download', 'Windows Download');
        return false;
    }
    if(selectedOS === 'Linux') {
        window.location.href ='https://d3gcsm2x428gz5.cloudfront.net/codepilot-ai-4.1.1-x86_64.AppImage';
        __gaTracker('send', 'event', 'Click', 'Download', 'Linux Download');
        return false;
    }
}

function detectOS() {
  var OSName = "MacOS"; //default to MacOS
  if (navigator.appVersion.indexOf("Win") != -1) OSName = "Windows";
  if (navigator.appVersion.indexOf("Mac") != -1) OSName = "MacOS";
  if (navigator.appVersion.indexOf("Linux") != -1) OSName = "Linux";
  return OSName;
}

// get user IP and trigger FOMO code on submit
var ipAddress;

function getIpAddress() {
   var xhttp = new XMLHttpRequest();
   xhttp.open("GET", "//freegeoip.net/json/?callback=?", true);
   xhttp.onreadystatechange = function() {
    if(xhttp.readyState === XMLHttpRequest.DONE && xhttp.status === 200) {
       var f = xhttp.responseText.search(/\(/)
       var l = xhttp.responseText.search(/\)/)
       data = JSON.parse(xhttp.responseText.slice(f + 1,l));
       ipAddress = data.ip;
    }
  }
  xhttp.send();
  console.log('ip: ' + ipAddress);
  }

  function fetchAndInjectIpAddress() {
   getIpAddress();

   var ipSetter = setInterval(function() {
     if (ipAddress && ipAddress.length > 0) {
     ipAddress
     clearInterval(ipSetter);
     }
     else {
       console.log('ip not found, retrying.');
     }
     }, 1000)
  }

function goFomo() {

   var email = jQuery('input[name="mauticform[email]"]').val();
   //this looked like the right element to me, feel free to modify for whatever works :)

   if (!email | !ipAddress) {return;}

   var params = {event: {
               email_address: email,
               ip_address: ipAddress,
               event_type_id: 55184
               // I added the e.type.id, this corresponds to your User signup template and is necessary to create events
             }
           }

   jQuery.ajax({
   url: 'https://www.usefomo.com/api/v1/applications/me/events',
   method: 'POST',
   contentType: 'application/json',
   headers: {
       "Authorization": "Token JF_wIiGvMh7_aIFEdHCJXA"
       //your API token is JF_wIiGvMh7_aIFEdHCJXA, can be found or regenerated within Fomo dashboard
   },
   data: JSON.stringify(params),
   success: function(data) {
       console.log("id: " + data.id);
       console.log(data.message);
     }
   })
}

document.addEventListener("DOMContentLoaded", function(event) {
fetchAndInjectIpAddress();
});


//attach event listener to input
jQuery('#mauticform_acceptlicense').on('submit', goFomo)
